{
  "name": "@v-charts2/radar",
  "version": "1.1.1",
  "description": "echarts 雷达图",
  "keywords": [
    "echarts",
    "v-echarts",
    "v-charts",
    "radar"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./vue3": {
      "import": "./lib/vue3/v-charts.radar.mjs",
      "require": "./lib/vue3/v-charts.radar.umd.js"
    },
    "./vue2": {
      "import": "./lib/vue2/v-charts.radar.mjs",
      "require": "./lib/vue2/v-charts.radar.umd.js"
    },
    "./main": "./src/main.js"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "build:lib": "pnpm run clean && pnpm run build:lib-all",
    "build:lib-all": "pnpm run build:lib-vue3 && pnpm run build:lib-vue2 && pnpm run copy",
    "build:lib-vue2": "vite build --mode vue2",
    "build:lib-vue3": "vite build --mode vue3",
    "clean": "npx rimraf lib",
    "copy": "cpx \"../../CHANGELOG.md\" \"./\""
  },
  "dependencies": {
    "@v-charts2/core": "workspace:*",
    "@v-charts2/pie": "workspace:*",
    "echarts": "^5.4.1"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup-plugin-copy": "^3.4.0",
    "vite": "^4.0.1",
    "vite-plugin-banner": "^0.7.0"
  },
  "peerDependencies": {
    "echarts": ">5.0.0",
    "vue": ">2.6.0 || >3.2.25"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "b57fb0006cfeb85b8cccd2ca1e9bf488686d71f6"
}
